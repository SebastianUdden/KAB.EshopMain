<body>
    <header>
        <div class="containerLeft">
            <nav class="navbar navbar-default">
                <div ng-controller="headerController">
                    <div class="container-fluid">
                        <br />
                        <div class="nav navbar-nav navbar-left">
                            <div class="navbar-header">
                                <a class="navbar-brand" href="~/home/index" style="font-size:40px">KAB <span class="glyphicon glyphicon-home"></span></a>
                            </div>
                            <div class="col-md-6 col-md-offset-3">
                                <div class="input-group">
                                    <input type="text" class="form-control" ng-model="searchResult" />
                                    <span class="input-group-btn">
                                        <a href="#/search-results" class="btn btn-primary">
                                            <span style="clear:both" class="glyphicon glyphicon-search"></span>
                                        </a>
                                    </span>
                                </div>
                            </div>
                        </div>
                    </div>
                     
                    <ul class="nav navbar-nav navbar-right">
                        <li><a href="#"><span class="glyphicon glyphicon-globe"></span> Nyheter</a></li>
                        <li><a href="#/shopping-cart"><span class="glyphicon glyphicon-shopping-cart"></span> Kundvagn</a></li>
                        <li><a href="~/products/createproduct"><span class="glyphicon glyphicon-link"></span> Produktregistrering</a></li>
                        @if(Context.Request.Cookies["FirstName"].Count != 0)
                        {
                            <li><a><span class="glyphicon glyphicon-briefcase"></span> welcome @Context.Request.Cookies["FirstName"].First() </a></li>
                        }
                        else
                        {
                            <li><a href="~/customers/create"><span class="glyphicon glyphicon-user"></span> Registrering</a></li>
                            <li><a href="~/customers/login"><span class="glyphicon glyphicon-log-in"></span> Inloggning </a></li>
                        }
                        <li><a href="#/iterations"><span class="glyphicon glyphicon-cog"></span> Utvecklingsfaser </a></li>
                    </ul>
                </div>
            </nav>
        </div>
    </header>

    <div class="container">
        @RenderBody()
    </div>

    <nav class="navbar navbar-inverse sidebar-left" role="navigation">
        <div class="container">
            <div id="wrapper">
                <ul class="nav navbar-nav navbar-right">
                    <li><a href="~/products/productcategory/2"><span class="glyphicon glyphicon-pencil"></span> Pennor</a></li>
                    <li><a href="~/products/productcategory/3"><span class="glyphicon glyphicon-envelope"></span> Papper</a></li>
                    <li><a href="~/products/productcategory/4"><span class="glyphicon glyphicon-paperclip"></span> Skrivare</a></li>
                    <li><a href="~/products/productcategory/5"><span class="glyphicon glyphicon-cutlery"></span> Kök</a></li>
                    <li><a href="~/products/productcategory/6"><span class="glyphicon glyphicon-globe"></span> Övrigt</a></li>
        </ul>
    </div>
        </div>
    </nav>


