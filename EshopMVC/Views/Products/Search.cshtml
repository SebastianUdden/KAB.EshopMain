@model ProductListViewModel[]
<div class="container">
    <div class="row">
        <div class="col-xs-12 col-sm-8 col-md-6 col-xs-offset-1">
            <table>
                <tr>
                    <th>
                        Sökresultat
                    </th>
                </tr>
                @foreach (var item in Model)
                {
                    <tr>
                        <td><a href="~/products/product/@item.Id">@item.ProductName</a></td>
                    </tr>
                }
            </table>
        </div>
    </div>
</div>